	funcao vazio escreve(cadeia texto)
	{
		texto+=" "
		inteiro nc = t.numero_caracteres(texto)
		
		
		cadeia temp = ""
		inteiro base = 0
		para(inteiro i = 0; i < nc; i++) //para cada caractere no texto
		{
			caracter sera = t.obter_caracter(texto, i)
			se(sera == ' ')
			{
				se(i == 1)
					temp = t.extrair_subtexto(texto, base, (i))
				se(i > 1)
					temp = t.extrair_subtexto(texto, base, (i))
				base = i + 1
				escreva(temp)

				
				se(t.numero_caracteres(temp) == 0)
					tempo(1)
				se(t.numero_caracteres(temp) == 1 ou t.numero_caracteres(temp) == 2 ou t.numero_caracteres(temp) == 3)
					tempo(190)
				se(t.numero_caracteres(temp) == 4 ou t.numero_caracteres(temp) == 5 ou t.numero_caracteres(temp) == 6)
					tempo(290)
				se(t.numero_caracteres(temp) >= 7)
					tempo(390)
				

				
				escreva(" ")
			}
			
		}
		
	}


